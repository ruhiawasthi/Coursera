    "\n",
    "def computepay(h,r):\n",
    "    if h<= 40:\n",
    "           return h*r\n",
    "    else:\n",
    "           return (40*r)+(h-40)*1.5*r\n",
    "    \n",
    "hrs = input('Enter Hours:')\n",
    "hr = float(hrs)\n",
    "rate = input('Enter rate:')\n",
    "rt = float(rate)\n",
    "\n",
    "pay = computepay(hr,rt)\n",
    "print(\"Pay\" ,pay)"
   ]
  }
